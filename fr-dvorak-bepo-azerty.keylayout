    <action id="abovedot">
      <when state="none" next="abovedot"/>
      <when state="acute" next="abovedot_acute"/>
      <when state="caron" next="abovedot_caron"/>
      <when state="macron" next="abovedot_macron"/>
    </action>
    <action id="acute">
      <when state="none" next="acute"/>
      <when state="abovedot" next="abovedot_acute"/>
      <when state="breve" next="acute_breve"/>
      <when state="cedilla" next="acute_cedilla"/>
      <when state="circumflex" next="acute_circumflex"/>
      <when state="diaresis" next="acute_diaresis"/>
      <when state="macron" next="acute_macron"/>
      <when state="ringabove" next="acute_ringabove"/>
      <when state="stroke" next="acute_stroke"/>
      <when state="tilde" next="acute_tilde"/>
    </action>
    <action id="breve">
      <when state="none" next="breve"/>
      <when state="acute" next="acute_breve"/>
      <when state="cedilla" next="breve_cedilla"/>
      <when state="grave" next="breve_grave"/>
      <when state="tilde" next="breve_tilde"/>
    </action>
    <action id="caron">
      <when state="none" next="caron"/>
      <when state="abovedot" next="abovedot_caron"/>
      <when state="diaresis" next="caron_diaresis"/>
    </action>
    <action id="cedilla">
      <when state="none" next="cedilla"/>
      <when state="acute" next="acute_cedilla"/>
      <when state="breve" next="breve_cedilla"/>
    </action>
    <action id="circumflex">
      <when state="none" next="circumflex"/>
      <when state="acute" next="acute_circumflex"/>
      <when state="grave" next="circumflex_grave"/>
      <when state="tilde" next="circumflex_tilde"/>
    </action>
    <action id="diaresis">
      <when state="none" next="diaresis"/>
      <when state="acute" next="acute_diaresis"/>
      <when state="caron" next="caron_diaresis"/>
      <when state="grave" next="diaresis_grave"/>
      <when state="macron" next="diaresis_macron"/>
      <when state="tilde" next="diaresis_tilde"/>
    </action>
    <action id="doubleacute">
      <when state="none" next="doubleacute"/>
    </action>
    <action id="grave">
      <when state="none" next="grave"/>
      <when state="breve" next="breve_grave"/>
      <when state="circumflex" next="circumflex_grave"/>
      <when state="diaresis" next="diaresis_grave"/>
      <when state="macron" next="grave_macron"/>
      <when state="tilde" next="grave_tilde"/>
    </action>
    <action id="macron">
      <when state="none" next="macron"/>
      <when state="abovedot" next="abovedot_macron"/>
      <when state="acute" next="acute_macron"/>
      <when state="diaresis" next="diaresis_macron"/>
      <when state="grave" next="grave_macron"/>
      <when state="ogonek" next="macron_ogonek"/>
      <when state="tilde" next="macron_tilde"/>
    </action>
    <action id="ringabove">
      <when state="none" next="ringabove"/>
      <when state="acute" next="acute_ringabove"/>
    </action>
    <action id="ogonek">
      <when state="none" next="ogonek"/>
      <when state="macron" next="macron_ogonek"/>
    </action>
    <action id="stroke">
      <when state="none" next="stroke"/>
      <when state="acute" next="acute_stroke"/>
    </action>
    <action id="tilde">
      <when state="none" next="tilde"/>
      <when state="acute" next="acute_tilde"/>
      <when state="breve" next="breve_tilde"/>
      <when state="circumflex" next="circumflex_tilde"/>
      <when state="diaresis" next="diaresis_tilde"/>
      <when state="grave" next="grave_tilde"/>
      <when state="macron" next="macron_tilde"/>
    </action>
    
  </actions>


  <terminators>
    <when state="abovedot" output="˙"/>
    <when state="acute" output="´"/>
    <when state="breve" output="̆"/>
    <when state="caron" output="ˇ"/>
    <when state="cedilla" output="¸"/>
    <when state="circumflex" output="^"/>
    <when state="diaresis" output="¨"/>
    <when state="doubleacute" output="˝"/>
    <when state="grave" output="`"/>
    <when state="macron" output="¯"/>
    <when state="ogonek" output="˛"/>
    <when state="stroke" output="-"/>
    <when state="ringabove" output="°"/>
    <when state="tilde" output="˜"/>
    <!-- les doubles touches mortes -->
    <when state="abovedot_acute" output="˙´"/>
    <when state="abovedot_caron" output="˙ˇ"/>
    <when state="abovedot_macron" output="˙¯"/>
    <when state="acute_breve" output="´˘"/>
