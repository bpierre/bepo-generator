    <when state="breve_cedilla" output="˘¸"/>
    <when state="breve_grave" output="˘`"/>
    <when state="breve_tilde" output="˘~"/>
    <when state="caron_diaresis" output="ˇ¨"/>
    <when state="circumflex_grave" output="^`"/>
    <when state="circumflex_tilde" output="^~"/>
